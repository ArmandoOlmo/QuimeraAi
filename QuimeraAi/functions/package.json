    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.10.0",
    "@google-cloud/text-to-speech": "^5.0.0",
    "@google/genai": "^1.31.0",
    "@google/generative-ai": "^0.24.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0",
    "resend": "^2.1.0",
    "stripe": "^15.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "private": true
}