{
  "name": "quimera-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for Quimera AI",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
